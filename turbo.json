{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "generate": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false,
      "persistent": true
    },
    "push": {
      "cache": false,
      "persistent": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "seed": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "outputs": ["coverage/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    },
    "format": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": false,
      "persistent": true
    }
  }
}
