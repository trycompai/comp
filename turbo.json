{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "globalEnv": [
    "AI_GATEWAY_API_KEY",
    "ANTHROPIC_API_KEY",
    "APP_AWS_ACCESS_KEY_ID",
    "APP_AWS_BUCKET_NAME",
    "APP_AWS_KNOWLEDGE_BASE_BUCKET",
    "APP_AWS_ORG_ASSETS_BUCKET",
    "APP_AWS_QUESTIONNAIRE_UPLOAD_BUCKET",
    "APP_AWS_REGION",
    "APP_AWS_SECRET_ACCESS_KEY",
    "AUTH_GITHUB_ID",
    "AUTH_GITHUB_SECRET",
    "AUTH_GOOGLE_ID",
    "AUTH_GOOGLE_SECRET",
    "AUTH_MICROSOFT_CLIENT_ID",
    "AUTH_MICROSOFT_CLIENT_SECRET",
    "AUTH_SECRET",
    "AUTH_TRUSTED_ORIGINS",
    "BETTER_AUTH_SECRET",
    "DATABASE_URL",
    "DISCORD_WEBHOOK_URL",
    "DUB_API_KEY",
    "DUB_PROGRAM_ID",
    "DUB_REFER_URL",
    "ENTERPRISE_API_SECRET",
    "EXA_API_KEY",
    "FIRECRAWL_API_KEY",
    "FLEET_DEVICE_PATH_MAC",
    "FLEET_DEVICE_PATH_WINDOWS",
    "FLEET_TOKEN",
    "FLEET_URL",
    "GROQ_API_KEY",
    "INTERNAL_API_TOKEN",
    "LOGO_DEV",
    "LOGTAIL_SOURCE_TOKEN",
    "LOGTAIL_URL",
    "NODE_ENV",
    "NOVU_API_KEY",
    "OPENAI_API_KEY",
    "QA_SECRET",
    "QUESTIONNAIRE_AUTOFILL_FILE_API_SECRET",
    "RESEND_API_KEY",
    "RESEND_AUDIENCE_ID",
    "RESEND_DOMAIN",
    "RESEND_FROM_DEFAULT",
    "RESEND_FROM_MARKETING",
    "RESEND_FROM_SYSTEM",
    "RESEND_REPLY_TO_MARKETING",
    "RESEND_TO_TEST",
    "RETOOL_COMP_API_SECRET",
    "REVALIDATION_SECRET",
    "SECRET_KEY",
    "SKIP_ENV_VALIDATION",
    "SLACK_SALES_WEBHOOK",
    "TASKS_AUTOMATION_BUCKET",
    "TRUST_PORTAL_PROJECT_ID",
    "UPSTASH_REDIS_REST_TOKEN",
    "UPSTASH_REDIS_REST_URL",
    "UPSTASH_VECTOR_REST_TOKEN",
    "UPSTASH_VECTOR_REST_URL",
    "ZAPIER_HUBSPOT_WEBHOOK_URL"
  ],
  "ui": "tui",
  "tasks": {
    "db:generate": {
      "cache": false,
      "outputs": ["prisma/schema.prisma", "node_modules/.prisma/**"]
    },
    "build": {
      "dependsOn": ["^build", "db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typecheck": {
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "deps:check": {
      "cache": false
    },
    "test": {
      "cache": false,
      "outputs": ["coverage/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    }
  }
}
